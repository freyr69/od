<div class="row">
    <div class="col-md-6 col-md-offset-3 text-center">
        <div class="widget">

            <div class="header">
                <h3>Stats</h3>

                <ul class="list-inline">
                    <li class=""><a href="/stats/orgasm">Orgasms</a></li>
                    <li class=""><a href="/stats/chastity">Chastity</a></li>
                    <li class="active"><a href="/stats/arousal">Arousal</a></li>
                </ul>
            </div>


            <div class="toppad2">
                <div class="table-responsive">
                    <table class="table table-bordered table-striped table-condensed">
                        <thead>
                            <tr>
                                <th>Date</th>
                                <th>Level</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% _.each(log, function(l) { %>
                            <tr>
                                <td><%= l.formattedDate() %></td>
                                <td><%= l.level %></td>
                            </tr>
                            <% }) %>
                        </tbody>

                    </table>    
                </div>
            </div>
        </div>
    </div>
</div>