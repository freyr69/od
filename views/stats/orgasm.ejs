<div class="row">
    <div class="col-md-6 col-md-offset-3 text-center">
        <div class="widget">

            <div class="header">
                <h3>Stats</h3>

                <ul class="nav nav-pills">
                    <li class="active"><a href="/stats/orgasm">Orgasms</a></li>
                    <li class=""><a href="/stats/chastity">Chastity</a></li>
                </ul>
            </div>


            <div class="toppad2">
                <div class="table-responsive">
                    <table class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Type</th>
                                <th>DateTime</th>
                                <th>Time Since</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% _.each(log, function(l) { %>
                            <tr>
                                <td><%= l.typeName() %></td>
                                <td><%= l.formattedDate() %></td>
                                <td><%= moment(l.date).fromNow() %></td>
                            </tr>
                            <% }) %>
                        </tbody>

                    </table>    
                </div>
            </div>
        </div>
    </div>
</div>