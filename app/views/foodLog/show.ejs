<h2 class="text-info">Details</h2>
<fieldset>
    <legend>foodLog</legend>

	
		<label for="title">title:</label>
		<p><%- foodLog.title %></p>
	
		<label for="points">points:</label>
		<p><%- foodLog.points %></p>
	
		<label for="date">date:</label>
		<p><%- moment.utc(foodLog.date).tz('America/New_York').format('dddd, MMMM Do YYYY, h:mm:ss a') %></p>
	

<%- link_to('edit', '/foodLog/edit/'+foodLog.id, {class : 'btn btn-warning'}) %>
</fieldset>

<%- link_to('back to list', '/foodLog', {class : 'btn btn-primary'}) %>
