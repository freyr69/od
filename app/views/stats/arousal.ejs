<div class="row">
    <div class="col-md-6 col-md-offset-3 text-center">
        <div class="widget">

            <div class="header">
                <h3>Stats - Arousal</h3>

                <ul class="list-inline">
                    <li class=""><a href="/stats/orgasm">Orgasms</a></li>
                    <li class=""><a href="/stats/chastity">Chastity</a></li>
                    <li class="active"><a href="/stats/arousal">Arousal</a></li>
                    <li class=""><a href="/stats/punishment">Punishments</a></li>
                    <li class=""><a href="/stats/task">Tasks</a></li>
                    <li class=""><a href="/stats/tease">Teases</a></li>
                </ul>
            </div>


            <div class="toppad2">
                <div class="table-responsive">
                    <table class="table table-bordered table-striped table-condensed">
                        <thead>
                            <tr>
                                <th>Date</th>
                                <th>Level</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% _.each(data, function(l) { %>

                            <% 

                            var code = "default";
                            switch(l.level) { 
                                case 1:
                                    code = "default";
                                    break;
                                case 2:
                                    code = "primary";
                                    break;
                                case 3:
                                    code = "info";
                                    break;
                                case 4:
                                    code = "warning";
                                    break;
                                case 5:
                                    code = "danger";
                                    break;
                            }    
                            %> 

                            <tr>
                                <td><%= moment.utc(l.date).tz('America/New_York').format('dddd, MMMM Do YYYY, h:mm:ss a') %></td>
                                <td><span class="label label-<%= code %>"><%= l.level %></span></td>
                            </tr>
                            <% }) %>
                        </tbody>

                    </table>    
                </div>
            </div>
        </div>
    </div>
</div>