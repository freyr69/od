<div class="row">
    <div class="col-md-6 col-md-offset-3 text-center">
        <div class="widget">

            <div class="header">
                <h3>Stats - Chastity</h3>

                <ul class="list-inline">
                    <li class=""><a href="/stats/orgasm">Orgasms</a></li>
                    <li class=""><a href="/stats/chastity">Chastity</a></li>
                    <li class=""><a href="/stats/arousal">Arousal</a></li>
                    <li class=""><a href="/stats/punishment">Punishments</a></li>
                    <li class=""><a href="/stats/task">Tasks</a></li>
                    <li class="active"><a href="/stats/tease">Teases</a></li>
                </ul>
            </div>
<div class="toppad2">
                <div class="table-responsive">
                    <table class="table table-bordered table-striped table-condensed">
                        <thead>
                            <tr>
                                <th>Title</th>
                                <th>Description</th>
                                <th>Severity</th>
                                <th>Assigned</th>
                                <th>In Chastity</th>
                                <th>Start</th>
                                <th>End</th>
                                <th>Duration</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% _.each(data, function(l) { %>
                            <tr>
                                <td><%= l.title %></td>
                                <td><%= l.description %></td>
                                <td><%= l.severity %></td>
                                <td><%= moment.utc(l.assigned).tz('America/New_York').format('dddd, MMMM Do YYYY, h:mm:ss a') %></td>
                                <td>
                                    <% if (l.inChasity) { %>
                                        <i class="fa fa-check-square-o"></i>
                                    <% } %>
                                </td>
                                <td><%= moment.utc(l.start).tz('America/New_York').format('dddd, MMMM Do YYYY, h:mm:ss a') %></td>
                                <td>
                                    <% if(l.end) { %>
                                        <%= moment.utc(l.end).tz('America/New_York').format('dddd, MMMM Do YYYY, h:mm:ss a') %></td>
                                    <% } else { %>
                                    ------
                                    <% } %>
                                <td>
                                    <% if(l.end) { %>
                                    <%= moment.duration(moment(l.end).diff(moment(l.start))).humanize() %>
                                    <% } else { %>
                                    Ongoing
                                    <% } %>
                                </td>
                            </tr>
                            <% }) %>
                        </tbody>

                    </table>    
                </div>
            </div>
        </div>
    </div>
</div>